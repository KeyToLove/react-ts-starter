{
  "name": "react-ts-starter",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development BASE_ENV=dev webpack-dev-server -c build/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=development BASE_ENV=test webpack-dev-server -c build/webpack.dev.js",
    "start:uat": "cross-env NODE_ENV=development BASE_ENV=uat webpack-dev-server -c build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack -c build/webpack.prod.js",
    "build:github": "cross-env NODE_ENV=production BASE_NAME=/react-ts-starter webpack -c build/webpack.prod.js",
    "build:analyse": "webpack -c build/webpack.analyse.js",
    "lint": "eslint --ext  .js,.ts,.tsx src",
    "lint:style": "stylelint src/**/*.{css,less}",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src"
  },
  "author": "KeyToLove<coderEDC@163.com>",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write",
      "npm run lint:fix"
    ],
    "*.{css,less}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.31.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.0",
    "react-dev-inspector": "^1.8.6",
    "react-refresh": "^0.14.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "thread-loader": "^4.0.2",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "repository": "https://github.com/KeyToLove/react-ts-starter",
  "dependencies": {
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  }
}
